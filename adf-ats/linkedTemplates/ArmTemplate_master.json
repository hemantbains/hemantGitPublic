{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ats"
		},
		"LS_ADLSg2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSg2'"
		},
		"LS_AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_AzureDatabricks1'"
		},
		"LS_Dbfs_bifurcate_SubSmry_filesource_name_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_Dbfs_bifurcate_SubSmry_filesource_name'"
		},
		"LS_dbfs2_bifurcate_stu_sourcefilepath_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_dbfs2_bifurcate_stu_sourcefilepath'"
		},
		"ls_Src_AzureSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_Src_AzureSQLDB'"
		},
		"ls_adlsg2edls_excel_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsg2edls_excel'"
		},
		"ls_adlsg2edls_excel_SubSmry_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsg2edls_excel_SubSmry'"
		},
		"sink_AzureSqlDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sink_AzureSqlDb'"
		},
		"LS_ADLSg2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsg2edls.dfs.core.windows.net/"
		},
		"LS_AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1212-065554-24xhgg4x"
		},
		"LS_Dbfs_bifurcate_SubSmry_filesource_name_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1212-065554-24xhgg4x"
		},
		"LS_dbfs2_bifurcate_stu_sourcefilepath_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1212-065554-24xhgg4x"
		},
		"ls_adlsg2edls_excel_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsg2edls.dfs.core.windows.net/"
		},
		"ls_adlsg2edls_excel_SubSmry_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsg2edls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ats_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSg2_accountKey": {
						"value": "[parameters('LS_ADLSg2_accountKey')]"
					},
					"LS_AzureDatabricks1_accessToken": {
						"value": "[parameters('LS_AzureDatabricks1_accessToken')]"
					},
					"LS_Dbfs_bifurcate_SubSmry_filesource_name_accessToken": {
						"value": "[parameters('LS_Dbfs_bifurcate_SubSmry_filesource_name_accessToken')]"
					},
					"LS_dbfs2_bifurcate_stu_sourcefilepath_accessToken": {
						"value": "[parameters('LS_dbfs2_bifurcate_stu_sourcefilepath_accessToken')]"
					},
					"ls_Src_AzureSQLDB_connectionString": {
						"value": "[parameters('ls_Src_AzureSQLDB_connectionString')]"
					},
					"ls_adlsg2edls_excel_accountKey": {
						"value": "[parameters('ls_adlsg2edls_excel_accountKey')]"
					},
					"ls_adlsg2edls_excel_SubSmry_accountKey": {
						"value": "[parameters('ls_adlsg2edls_excel_SubSmry_accountKey')]"
					},
					"sink_AzureSqlDb_connectionString": {
						"value": "[parameters('sink_AzureSqlDb_connectionString')]"
					},
					"LS_ADLSg2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSg2_properties_typeProperties_url')]"
					},
					"LS_AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"LS_Dbfs_bifurcate_SubSmry_filesource_name_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_Dbfs_bifurcate_SubSmry_filesource_name_properties_typeProperties_existingClusterId')]"
					},
					"LS_dbfs2_bifurcate_stu_sourcefilepath_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_dbfs2_bifurcate_stu_sourcefilepath_properties_typeProperties_existingClusterId')]"
					},
					"ls_adlsg2edls_excel_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsg2edls_excel_properties_typeProperties_url')]"
					},
					"ls_adlsg2edls_excel_SubSmry_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsg2edls_excel_SubSmry_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}