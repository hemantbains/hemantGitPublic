{
	"name": "ppl_load_Excel_GSS",
	"properties": {
		"description": "Pipeline to store Global Super Store excel multiple sheets from Azure Blob Storage to Azure SQL DB",
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.Parameter_Sheetname",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Acti_CopyExcelGSS",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "hs-input//global_superstore_2016.xlsx"
								}
							],
							"typeProperties": {
								"source": {
									"type": "ExcelSource",
									"storeSettings": {
										"type": "AzureBlobStorageReadSettings",
										"recursive": {
											"value": "@item().Parameter_Sheetname",
											"type": "Expression"
										},
										"enablePartitionDiscovery": false
									}
								},
								"sink": {
									"type": "AzureSqlSink",
									"writeBehavior": "insert",
									"sqlWriterUseTableLock": false,
									"tableOption": "autoCreate",
									"disableMetricsCollection": false
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "Dataset_global_super_store_xlsx",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "AzureSqlTable1",
									"type": "DatasetReference",
									"parameters": {
										"Pm_Sink_Tablename": {
											"value": "@item().Parameter_Sheetname",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"Parameter_Sheetname": {
				"type": "array",
				"defaultValue": [
					{
						"Sheetname": "Orders"
					},
					{
						"Sheetname": "Returns"
					},
					{
						"Sheetname": "People"
					}
				]
			}
		},
		"annotations": []
	}
}